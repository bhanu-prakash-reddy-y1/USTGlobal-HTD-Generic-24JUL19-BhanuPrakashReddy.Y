package com.ustglobal.searching;

public class UnorderedLinearSearch {

	public static void main(String[] args) {
		int [] arr= {1,2,3,4,5,6,7};

	}

	public static boolean LinerSearch(int[] arr,int value) {
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] == value) {
				
				return true;
				
			}
		}
		
		return false;
	}
}
