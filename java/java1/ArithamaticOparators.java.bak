class ArithamaticOparators 
{
	public static void main(String[] args) 
	{
		int i = 10;
		int j = 20;
		int res = i * j;//multiplication & reintilization
		System.out.println(" * Result = "+res);
		int a = 1234;
		int b = 212;
		res = a + b;//addition & reintilization
		System.out.println(" + Result = "+res);
		int c = 1123;
		int d = 20123;
		res = c - d;//subtraction & reintilization
		System.out.println(" - Result = "+res);
		int e = 10123;
		int f = 20123;
		res = f / e;//division & reintilization
		System.out.println(" / Result = "+res);
		int g= 101;
		int h=201;
		res = g % h;//modulus &  reintilization
		System.out.println(" % Result = "+res);
		char k = 'c';
		int l = 123;
		res = k+l;//char & reintiliztion
		System.out.println(" char Result = "+res);
	}
}
