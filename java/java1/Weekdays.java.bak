class Weekdays 
{
	public static void main(String[] args) 
	{
		int dayNum = 8;
		switch(dayNum){
			case 1:
			case 2:
			case 3:
			case 4:
			case 5:System.out.println("week day");
				break;
			case 6:
			case 7:System.out.println("week end");
				break;
			default:System.out.println("invalid dayNum");
		}
	}
}
