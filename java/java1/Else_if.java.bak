class Else_if 
{
	public static void main(String[] args) 
	{
		int i = 10;
		int j = 20;
		int k = 30;
		if( i < j){
		System.out.println("j  is greaterthen i");		}
		else if (j<k)
		{
			System.out.println("k is greaterthan j");
		}
		else {
			System.out.println(" i is greaterthan both j and k");
		}

		System.out.println("Hello World!");
	}
}
